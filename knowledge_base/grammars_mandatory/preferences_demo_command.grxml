    <one-of>
      <!-- Dialogue 3 -->
      <item>hi golem i like malz<tag>out.answer="b"</tag></item>
      <!-- Dialogue 6 -->
      <item>i like noodles best<tag>out.answer="b"</tag></item>
      <!-- Dialogue 9 -->
      <item>no thanks golem that is ok<tag>out.answer="b"</tag></item>
      <!-- Dialogue 11 -->
      <item>i had a bad day<tag>out.answer="b"</tag></item>
