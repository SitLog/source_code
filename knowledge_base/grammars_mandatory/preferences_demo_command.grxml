      <item>bring me something to drink and bisquits<tag>out.answer="b"</tag></item>
      <!-- Dialogue 15  -->
      <item>yes<tag>out.answer="b"</tag></item>
      <!-- Dialogue 17  -->
      <item>no golem noodles is ok<tag>out.answer="b"</tag></item>
      <!-- Dialogue 28 and 32  -->
      <item>yes golem please<tag>out.answer="b"</tag></item>
    </one-of>
  </rule>
  
</grammar>
